{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "Your_Database_Connection_String_Here"
    },
    "ClosureAccountOptions": {
        "Enabled": true,
        "AgeThreshold": 30,
        "ProcessingDay": 1,
        "ProcessingMonth": 1,
        "TimeZone": "Asia/Singapore"
    }
}
